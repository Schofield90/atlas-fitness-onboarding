# Database
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Stripe Configuration (Optional - app will work without these but with limited functionality)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Other services
RESEND_API_KEY=your_resend_api_key
WEBHOOK_SECRET=your_webhook_secret

# Meta (Facebook) Messenger Integration
META_APP_ID=your_meta_app_id
META_APP_SECRET=your_meta_app_secret
META_VERIFY_TOKEN=your_meta_verify_token
META_GRAPH_VERSION=v18.0
META_OAUTH_REDIRECT_URI=https://your-app.com/api/integrations/meta/callback

# Feature Flags
NEXT_PUBLIC_ENABLE_BILLING_MSW_STUB=true
NEXT_PUBLIC_ENABLE_BILLING_RETRY_BUTTON=true