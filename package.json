{
  "name": "gymleadhub-onboarding",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.0.0",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:quick": "NODE_OPTIONS='--max-old-space-size=4096' next dev --turbo",
    "build": "next build",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_RUNTIME=nodejs next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit --incremental --tsBuildInfoFile .tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest tests/security",
    "test:api": "jest tests/api",
    "test:db": "jest tests/database",
    "test:perf": "jest tests/database/performance.test.ts",
    "test:integration": "tsx scripts/run-integration-tests.ts",
    "test:suite": "tsx scripts/run-tests.ts",
    "validate-db": "tsx scripts/validate-existing-db.ts",
    "db:fix": "tsx scripts/apply-db-fixes.ts",
    "db:health": "tsx scripts/database-health-report.ts",
    "db:benchmark": "tsx scripts/performance-benchmark.ts",
    "migration:validate": "tsx scripts/migration-validator.ts",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up",
    "dev:docker:build": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:sync": "./scripts/sync-vercel-env.sh",
    "dev:setup": "./scripts/dev-setup.sh",
    "dev:fast": "./scripts/sync-vercel-env.sh && npm run dev:turbo",
    "vercel:env:pull": "vercel env pull .env.local",
    "vercel:link": "vercel link",
    "workflow": "node scripts/dev-workflow.js",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "pre-deploy": "bash scripts/pre-deploy.sh",
    "debug-deploy": "bash scripts/debug-deploy.sh",
    "validate-env": "node scripts/validate-env.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@react-email/components": "^0.0.31",
    "@react-email/render": "^1.1.3",
    "@stripe/react-stripe-js": "^3.8.1",
    "@stripe/stripe-js": "^7.6.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.52.0",
    "@types/nodemailer": "^6.4.17",
    "@types/papaparse": "^5.3.16",
    "bullmq": "^5.56.5",
    "clsx": "^2.1.1",
    "cron-parser": "^5.3.0",
    "date-fns": "^4.1.0",
    "google-auth-library": "^10.2.0",
    "googleapis": "^154.0.0",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.263.1",
    "moment": "^2.30.1",
    "next": "15.3.5",
    "nodemailer": "^7.0.5",
    "openai": "^5.10.2",
    "papaparse": "^5.5.3",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-big-calendar": "^1.19.4",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18",
    "reactflow": "^11.11.4",
    "recharts": "^3.1.0",
    "resend": "^4.7.0",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "twilio": "^5.7.3",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@prisma/client": "^6.13.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.1",
    "eslint": "^8",
    "eslint-config-next": "15.3.5",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "ignore-loader": "^0.1.2",
    "jest": "^30.0.5",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "null-loader": "^4.0.1",
    "postcss": "^8",
    "prisma": "^6.13.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
