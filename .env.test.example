# E2E Testing Environment Variables
# Copy this to .env.test and update values

# Allow test login route (required for E2E)
ALLOW_TEST_LOGIN=true

# Test user credentials
TEST_SUPERADMIN_EMAIL=superadmin@test.example.com
TEST_OWNER_EMAIL=owner@test.example.com
TEST_MEMBER_EMAIL=member@test.example.com
TEST_USER_PASSWORD=TestPassword123!

# Supabase Configuration (use your actual values)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_PROJECT_ID=your-project-id

# Optional: Test organization settings
TEST_ORGANIZATION_NAME=Test Gym
TEST_ORGANIZATION_ID=test-org-id